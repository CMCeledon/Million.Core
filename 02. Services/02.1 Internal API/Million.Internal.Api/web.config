<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="." inheritInChildApplications="false">
		<system.webServer>
			<handlers>
				<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
			</handlers>
			<aspNetCore processPath="dotnet" arguments=".\Million.Internal.Api.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
			<httpProtocol>
				<customHeaders>
					<remove name="X-Powered-By" />
					<add name="Content-Security-Policy" value="default-src: https:; frame-ancestors 'self' X-Frame-Options: SAMEORIGIN" />
					<!--<add name="Content-Security-Policy" value="default-src 'self'" />-->
					<add name="X-Frame-Options" value="SAMEORIGIN" />
					<add name="Cache-Control" value="no-store" />
					<add name="Pragma" value="no-cache" />
				</customHeaders>
			</httpProtocol>
			<rewrite>
				<outboundRules>
					<rule name="Eliminar Server Header">
						<match serverVariable="RESPONSE_SERVER" pattern=".*" />
						<action type="Rewrite" />
					</rule>
				</outboundRules>
			</rewrite>
      <security>
        <requestFiltering>
          <requestLimits maxAllowedContentLength="314572800" />
        </requestFiltering>
      </security>
		</system.webServer>
	</location>
</configuration>
